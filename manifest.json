{
    "manifest_version": 3,
    "name": "ExploitScan",
    "version": "1.8",
    "description": "Scans for PHPUnit CVE-2017-9841, Object Injection, SQL Injection, XSS, outdated Nginx, and outdated PHP",
    "permissions": ["scripting", "tabs", "activeTab"],
    "host_permissions": ["http://*/*", "https://*/*"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    }
  }
  